<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <dev>
            <magenizr_debugger>
                <enabled>1</enabled>
                <download_logs_reports_allow>1</download_logs_reports_allow>
                <report_display_latest>1</report_display_latest>
                <php_display_version>1</php_display_version>
                <restriction_ip></restriction_ip>
            </magenizr_debugger>
        </dev>
    </default>
</config>